# Minimal package config for sawPSMove (mirrors sawOptoforceSensor style)

set (sawPSMove_VERSION_MAJOR "@sawPSMove_VERSION_MAJOR@")
set (sawPSMove_VERSION_MINOR "@sawPSMove_VERSION_MINOR@")
set (sawPSMove_VERSION_PATCH "@sawPSMove_VERSION_PATCH@")
set (sawPSMove_VERSION "@sawPSMove_VERSION@")

# When installed, this file sits in share/sawPSMove/.
# Compute install root and basic dirs.
get_filename_component(THIS_FILE_PATH "${CMAKE_CURRENT_LIST_FILE}" PATH)
get_filename_component(PKG_ROOT "${THIS_FILE_PATH}/../.." ABSOLUTE)

set (sawPSMove_INCLUDE_DIR "${PKG_ROOT}/include")
set (sawPSMove_LIBRARY_DIR "${PKG_ROOT}/lib")

# Library list to link against (may include sawPSMoveQt if built with Qt)
set (sawPSMove_LIBRARIES "@sawPSMove_LIBRARIES@")
